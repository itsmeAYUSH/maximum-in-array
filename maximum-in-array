public class maximum {
    public static void main(String args[]){
        int a[]={42,56,-11,68,-52,0,53,64};
        int max = a[0];
        for(int i=0 ; i<= a.length-1 ; i++){
            if(max < a[i]){
                max = a[i];
            }
        }
        System.out.println(max);
    }
}

